{
  "manifest_version": 3,
  "name": "Twitter Trend Hide",
  "version": "1.0.0",
  "description": "Twitterのトレンドを非表示にする拡張機能です",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["css/style.css"],
      "javascript": ["js/main.js"],
      "all_frames": true
    }
  ]
}
